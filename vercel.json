{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    },
    {
      "src":"/login",
      "dest":"/"
    },
        {
      "src":"/dashboard",
      "dest":"/"
    },
        {
      "src":"/appointments",
      "dest":"/"
    }
  ]
}

